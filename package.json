{
  "name": "gada-wms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prettier": "prettier --write src/**/*.{ts,tsx} **/*.{ts,tsx}",
    "lint": "eslint --fix src/**/*.{ts,tsx} **/*.{ts,tsx}",
